<!DOCTYPE html>
<html lang="fr">
<head>
    <meta charset="UTF-8">
    <title>Asera{% block title %} | Bienvenue{% endblock %}</title>
    <link rel="icon" type="image/png" href="{{ asset('images/logo/Cachet-type.png') }}">
    {% block stylesheets %}
        {{ encore_entry_link_tags('bo_entry') }}
    {% endblock %}

    {% block javascripts %}
        {{ encore_entry_script_tags('bo_entry') }}
    {% endblock %}
</head>
<body  class="with-welcome-text">
<div class="container-scroller">
    {% block main_page %}
        {% include 'backoffice/template/_nav_bar.html.twig' %}
        <div class="container-fluid page-body-wrapper">
            {% include 'backoffice/template/_theming.html.twig' %}
            {% include 'backoffice/template/_side_nav_right.html.twig' %}
            {% include 'backoffice/template/_side_nav.html.twig' %}
            <div class="main-panel">
                <div class="content-wrapper">
                    <div class="row">
                        {% for message in app.flashes('success') %}
                            <div class="alert alert-success">
                                {{ message }}
                            </div>
                        {% endfor %}
                        {% for message in app.flashes('error') %}
                            <div class="alert alert-danger">
                                {{ message }}
                            </div>
                        {% endfor %}
                        {% block body %}{% endblock %}
                    </div>
                </div>
            </div>
        </div>
        {% include 'backoffice/template/_footer.html.twig' %}
    {% endblock %}
</div>
</body>
</html>
